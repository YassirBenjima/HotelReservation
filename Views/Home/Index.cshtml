<!--Fact Counter-->
<section class="fact-counter" style="background-image:url(images/resource/228A6390.jpg);">
    <div class="container">
        <div class="row clearfix">
            <div class="counter-outer clearfix">
                <!-- Column: Happy Clients -->
                <article class="column counter-column col-md-3 col-sm-6 col-xs-12 wow fadeIn" data-wow-duration="0ms">
                    <div class="item">
                        <div class="icon"><i class="flaticon-happy"></i></div>
                        <div class="count-outer">
                            <span class="count-text" data-speed="6000" data-stop="@ViewBag.TotalClients">0</span>+
                        </div>
                        <h4 class="counter-title">Clients satisfaits</h4>
                    </div>
                </article>

                <!-- Column: Awards Winning (This one remains unchanged) -->
                <article class="column counter-column col-md-3 col-sm-6 col-xs-12 wow fadeIn" data-wow-duration="0ms">
                    <div class="item">
                        <div class="icon"><i class="flaticon-calendar"></i></div>
                        <div class="count-outer">
                            <span class="count-text" data-speed="6000" data-stop="@ViewBag.TotalReservations">0</span>
                        </div>
                        <h4 class="counter-title">Réservations</h4>
                    </div>
                </article>

                <!-- Column: Our Staff -->
                <article class="column counter-column col-md-3 col-sm-6 col-xs-12 wow fadeIn" data-wow-duration="0ms">
                    <div class="item">
                        <div class="icon"><i class="flaticon-network"></i></div>
                        <div class="count-outer">
                            <span class="count-text" data-speed="6000" data-stop="@ViewBag.TotalUsers">0</span>
                        </div>
                        <h4 class="counter-title">Notre équipe</h4>
                    </div>
                </article>

                <!-- Column: Total Rooms -->
                <article class="column counter-column col-md-3 col-sm-6 col-xs-12 wow fadeIn" data-wow-duration="0ms">
                    <div class="item">
                        <div class="icon"><i class="flaticon-bed"></i></div> <!-- Change icon to bed for rooms -->
                        <div class="count-outer">
                            <span class="count-text" data-speed="6000" data-stop="@ViewBag.TotalRooms">0</span>+
                        </div>
                        <h4 class="counter-title">Chambres</h4> <!-- Change text to Total Rooms -->
                    </div>
                </article>
            </div>

        </div>
    </div>
</section>


<!--Start our team area-->
<section class="our-team-area padd-2">
    <div class="container">
        <div class="sec-title center">
            <h2>Rencontrez <span>notre équipe</span></h2>
        </div>
        <div class="four-item-carousel owl-carousel owl-theme owl-nav-style-one owl-dot-none">

            @{
                var users = ViewBag.Users as List<User>;
                var imagesUsers = new List<string>
            {
            "images/team/1.jpg",
            "images/team/2.jpg",
            "images/team/3.jpg",
            "images/team/4.jpg"
            };
            }

            @if (users != null && users.Any())
            {
                int index = 0;
                @foreach (var user in users)
                {
                    <div class="single-team-member">
                        <figure class="img-box">
                            <img src="@imagesUsers[index % imagesUsers.Count]" alt="Image of @user.Username">
                            <div class="overlay">
                                <div class="inner-box">
                                    <ul class="social-style-three">
                                        <li><a href="#"><i class="fa fa-facebook"></i></a></li>
                                        <li><a href="#"><i class="fa fa-twitter"></i></a></li>
                                        <li><a href="#"><i class="fa fa-skype"></i></a></li>
                                    </ul>
                                </div>
                            </div>
                        </figure>
                        <div class="author-info text-center">
                            <h4>@user.Username</h4>
                            <a href="#"><p class="position">@user.Role</p></a>
                        </div>
                    </div>
                    index++;
                }
            }
            else
            {
                <p>No team members available</p>
            }
        </div>
    </div>
</section>

