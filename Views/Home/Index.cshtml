<!--Rooms Section-->
<section class="rooms-section padd-2" style="background-image:url(images/background/rb.jpg);">
    <div class="container">
        <!--Sec Title One-->
        <div class="sec-title center light">
            <h2><span>Chambre </span>d'hôtel</h2>
        </div>

        <div class="three-item-carousel">
            @{
                var rooms = ViewBag.Rooms as List<Room>;
            }

            @if (rooms != null && rooms.Any())
            {
                @foreach (var room in rooms)
                {
                    <div class="room-box">
                        <div class="inner-box">
                            <div class="image-box">
                                @if (room.Room_Image != null && room.Room_Image.Length > 0)
                                {
                                    <img src="data:image/jpeg;base64,@Convert.ToBase64String(room.Room_Image)" alt="Image of @room.RoomType" />
                                }
                                else
                                {
                                    <!-- Default Image if Room_Image is null -->
                                    <img src="images/resource/Pool Villa (general View).jpg" alt="Default Image" />
                                }
                            </div>

                            <div class="lower-content">
                                <div class="rating">
                                    <span class="fa fa-star"></span>
                                    <span class="fa fa-star"></span>
                                    <span class="fa fa-star"></span>
                                    <span class="fa fa-star"></span>
                                    <span class="fa fa-star"></span>
                                </div>
                                <h3><a href="single-room.html">@room.RoomType</a></h3>
                                <div class="price clearfix">
                                    @room.RoomPrice MAD
                                    <a href="@Url.Action("Book", "Rooms", new { id = room.RoomNumber })">Réserver</a>
                                </div>
                            </div>
                        </div>
                    </div>
                }
            }
            else
            {
                <p>No rooms available</p>
            }
        </div>
    </div>
</section>


<!--End Rooms Section-->
